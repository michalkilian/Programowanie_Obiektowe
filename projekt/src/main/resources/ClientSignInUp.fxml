<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.ClientSignInUp">
    <children>
        <Button fx:id="signInButton" layoutX="95.0" layoutY="288.0" mnemonicParsing="false" onAction="#signIn"
                text="Sign In"/>
        <TextField fx:id="signInUsername" layoutX="47.0" layoutY="194.0" promptText="username"/>
        <PasswordField fx:id="signInPassword" layoutX="47.0" layoutY="237.0" promptText="password"/>
        <TextField fx:id="signUpUsername" layoutX="591.0" layoutY="111.0" promptText="username"/>
        <TextField fx:id="signUpAuthorPseudonim" layoutX="591.0" layoutY="156.0" promptText="Your Autistic Pseudo"/>
        <PasswordField fx:id="signUpPassword" layoutX="591.0" layoutY="194.0" promptText="password"/>
        <PasswordField fx:id="signUpPasswordConfirm" layoutX="591.0" layoutY="237.0" promptText="Confrim password"/>
        <Button fx:id="signUpButton" layoutX="639.0" layoutY="288.0" mnemonicParsing="false" onAction="#signUp"
                text="Sign Up"/>
        <Button fx:id="backButton" layoutX="366.0" layoutY="526.0" mnemonicParsing="false" onAction="#goBackToCreate"
                prefHeight="35.0" prefWidth="69.0" text="Back"/>
    </children>
</AnchorPane>
